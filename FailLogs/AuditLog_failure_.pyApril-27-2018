CREATE TABLE jerrysjerseystore_myshopify_com (event_id int NOT NULL AUTO_INCREMENT , date  DATETIME, action VARCHAR(300) DEFAULT '-', method VARCHAR(300) DEFAULT '-', referringpage VARCHAR(300) DEFAULT '-', comment VARCHAR(300) DEFAULT '-', customer_id VARCHAR(300) DEFAULT '-', purpose VARCHAR(300) DEFAULT '-', status VARCHAR(300) DEFAULT '-', legal_basis VARCHAR(300) DEFAULT '-');
